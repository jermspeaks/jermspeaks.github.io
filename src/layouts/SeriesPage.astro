---
import FormattedDate from "../components/FormattedDate.astro";
import ProseArticle from "../components/ProseArticle.astro";
import type { CollectionEntry } from "astro:content";

type Props = CollectionEntry<"series">["data"];

const { createdDate, endDate } = Astro.props;
---

<ProseArticle>
  <FormattedDate date={createdDate} /> to
  {endDate ? <FormattedDate date={endDate} /> : "Present"}
  <slot />
</ProseArticle>
